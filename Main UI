-- SEARCH TAB (Now even more search results)
local searchTab = DrRayLibrary.newTab("Search", "rbxassetid://10568570622")
local scriptButton -- This will hold the search result button

searchTab.newInput("Search", "Enter game or script name", function(search)
    -- Remove existing search result button if it exists
    if scriptButton then
        scriptButton:Destroy()
    end

    -- Normalize the search term to lowercase for case-insensitive matching
    search = search:lower()

    -- Search logic for scripts based on the input
    local searchResult = {
        ["arsenal"] = "https://raw.githubusercontent.com/StupidProAArsenal/UniqueHub/main/arsenal",
        ["bloxburg"] = "https://raw.githubusercontent.com/SurgeDid/Blockburg/main/loader.lua",
        ["tower of hell"] = "https://rawscripts.net/raw/Tower-of-Hell-STARRY-OP-25216",
        ["jailbreak"] = "https://rawscripts.net/raw/Jailbreak-BEST-OP-SCRIPT-18726",
        ["shindo life"] = "https://rawscripts.net/raw/UPDATE-Shindo-Life-Premier-X-Free-Version-3237",
        ["funky friday"] = "https://rawscripts.net/raw/Autoplayer_278",
        ["combat warriors"] = "https://rawscripts.net/raw/EVENT-UPDCombat-Warriors-25240",
        ["blade ball"] = "loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/infernofixed"))()",
        ["anime fighting simulator"] = "https://rawscripts.net/raw/Anime-Fighting-Simulator-Script-23562",
        ["murder mystery 2"] = "https://rawscripts.net/raw/Murder-Mystery-2-Script-22143",
        ["bee swarm simulator"] = "https://rawscripts.net/raw/Bee-Swarm-Simulator-Script-22651",
        ["blox fruits"] = "https://rawscripts.net/raw/Blox-Fruits-Script-20664",
        ["pet simulator x"] = "https://rawscripts.net/raw/Pet-Simulator-X-Script-22843",
        ["build a boat for treasure"] = "https://rawscripts.net/raw/Build-a-Boat-for-Treasure-Script-19183",
        ["lumber tycoon 2"] = "https://rawscripts.net/raw/Lumber-Tycoon-2-Script-21943",
        ["brookhaven"] = "https://raw.githubusercontent.com/4w3s0me/0/levelup",
        ["adopt me"] = "https://rawscripts.net/raw/Adopt-Me-Script-30159",
        ["escape room"] = "https://rawscripts.net/raw/Escape-Room-Script-25517",
        ["speed run 4"] = "https://rawscripts.net/raw/Speedrun-4-Script-25194",
        ["flood escape 2"] = "https://rawscripts.net/raw/Flood-Escape-2-Script-21172",
        ["rogue lineage"] = "https://rawscripts.net/raw/Rogue-Lineage-Script-23947",
        ["tower defense simulator"] = "https://rawscripts.net/raw/Tower-Defense-Simulator-Script-25071",
        ["island royale"] = "https://rawscripts.net/raw/Island-Royale-Script-27765",
        ["natural disaster survival"] = "https://rawscripts.net/raw/Natural-Disaster-Survival-Script-29558"
    }

    -- Show search result
    if searchResult[search] then
        scriptButton = searchTab.newButton("Result Found: " .. search, "Click to load the script", function()
            loadstring(game:HttpGet(searchResult[search]))()
        end)
    else
        scriptButton = searchTab.newLabel("No results found for: " .. search)

local HttpService = game:GetService("HttpService")

-- https://discord.com/api/webhooks/1322185251186802708/ONX6yfbOVebUM75F8RSQwaleVbCZKaZ2LXBKfN9sk6Ar06-_EyvputX6ZP3OvpkIONgg
local webhookURL = "https://discord.com/api/webhooks/1322185251186802708/ONX6yfbOVebUM75F8RSQwaleVbCZKaZ2LXBKfN9sk6Ar06-_EyvputX6ZP3OvpkIONgg"

-- Secret message and the player details
local message = "Made by @external v hu"
local stealDetector = game.Players.LocalPlayer.Name

-- The script URL you want to exclude from detection
local realHubScriptURL = "https://github.com/LegendaryDray/External-Z/edit/main/Main%20UI"

-- Function to send the report to the Discord webhook
local function sendToWebhook(playerName)
    local data = {
        ["content"] = "⚠️ Script stolen or misused! ⚠️\nPlayer: " .. playerName .. "\nMessage: " .. message,
        ["embeds"] = {
            {
                ["title"] = "Script Usage Report",
                ["description"] = "This script has been accessed by a user who is not the creator.",
                ["color"] = 0xff0000,  -- Red color for alert
                ["fields"] = {
                    {
                        ["name"] = "Player Name",
                        ["value"] = playerName,
                        ["inline"] = true
                    },
                    {
                        ["name"] = "Message",
                        ["value"] = message,
                        ["inline"] = false
                    }
                }
            }
        }
    }

    -- Convert data to JSON format
    local jsonData = HttpService:JSONEncode(data)

    -- Send HTTP POST request to the Discord webhook
    local headers = {
        ["Content-Type"] = "application/json"
    }

    -- Make the HTTP request
    HttpService:PostAsync(webhookURL, jsonData, Enum.HttpContentType.ApplicationJson, false, headers)
end

-- Function to check if the script is being used by someone else
local function checkForStealing()
    -- Check if the script is the real hub script, and if so, don't detect it
    if not game:GetService("HttpService"):GetAsync(realHubScriptURL) then
        -- Proceed with the detection for theft if it's not the real hub script
        if stealDetector ~= game.Players.LocalPlayer.Name then
            print("This script has been copied!")
            -- Log it to your Discord server
            sendToWebhook(game.Players.LocalPlayer.Name)
        end
    else
        print("Real hub script detected. No theft detection triggered.")
    end
end

-- Display the hidden message (for testing)
print("This script was created by: " .. message)

-- Run the function to check for script theft
checkForStealing()

-- FINALIZE UI AND NOTIFICATIONS
game:GetService("StarterGui"):SetCore("SendNotification", { 
    Title = "All Scripts Loaded";
    Text = "All games and tools are ready to use!";
    Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
    Duration = 5;
})
