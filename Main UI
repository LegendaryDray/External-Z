-- Notifications
local StarterGui = game:GetService("StarterGui")

local notifications = {
    {
        Title = "📢 Please Wait.",
        Text = "External is currently launching, give us a second to authenticate you.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 60
    },
    {
        Title = "⚙️ Loading script up...",
        Text = "⏸️ Wait for it to load",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 60
    },
    {
        Title = "✅ Loaded successfully",
        Text = "😁 Enjoy",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
        Duration = 60
    }
}

for _, notification in ipairs(notifications) do
    StarterGui:SetCore("SendNotification", notification)
end

-- Load DrRay Library
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("External Z Hub x [V.1.0]", "Default")

